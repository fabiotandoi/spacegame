    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "install": "^0.13.0",
    "nodemon": "^3.1.3",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.2",
    "vite": "^5.3.2"
