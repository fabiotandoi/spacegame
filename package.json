{
  "name": "spacegame",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test-old": "echo \"Error: no test specified\" && exit 1",
    "build-old": "npx tsc && npx tsc-alias && npm run copy-files-to-public",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "copy-files-to-public": "npx copyfiles index.html public && npx copyfiles -a assets/**/* public",
    "watch": "npx nodemon --watch src --ext ts --exec \"npm run build-and-restart\"",
    "build-and-restart": "npm run build && node server/server.js",
    "build-dev": "npm run build && node server/server.js",
    "build-debug": "npm run watch",
    "install-clean": "npm install --no-audit --no-fund"
  },
